                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/Margin_padding12"
                    android:layout_marginTop="@dimen/Margin_padding4">

