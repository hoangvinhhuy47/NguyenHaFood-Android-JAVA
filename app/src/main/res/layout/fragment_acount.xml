                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/sanphamDX"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/sanphamDM">

                            <ImageView
                                android:layout_width="@dimen/width_icon_fragmentAccount"
                                android:layout_height="@dimen/heith_icon_fragmentAccount"
                                android:layout_gravity="center"
                                android:background="@drawable/eye_show"></ImageView>

                            <TextView
                                android:id="@id/top3"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/heith_textconten_fragmentAccount"
